generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}

model User {
  id         String   @id @default(uuid())
  fullName   String
  email      String   @unique
  password   String
  role       Role
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

enum Role {
  PATIENT
  DOCTOR
  STAFF
}