{
  "name": "ehr-backend",
  "version": "1.0.0",
  "description": "EHR Blockchain Backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "devEnv": "cross-env NODE_ENV=development node -r dotenv/config -r tsconfig-paths/register dist/server.js dotenv_config_path=.env.dev",
    "prodEnv": "cross-env NODE_ENV=production node -r dotenv/config -r tsconfig-paths/register dist/server.js dotenv_config_path=.env",
    "production": "cross-env NODE_ENV=production node -r tsconfig-paths/register dist/server.js",
    "build": "rimraf dist && tsc",
    "build:production": "rimraf dist && tsc && node scripts/build.js",
    "prisma:generate": "cross-env NODE_ENV=development prisma generate",
    "prisma:migrate": "cross-env NODE_ENV=development prisma migrate dev",
    "prisma:db-push": "cross-env NODE_ENV=development prisma db push",
    "prisma:studio": "cross-env NODE_ENV=development prisma studio",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ehr",
    "blockchain",
    "healthcare",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/helmet": "^4.0.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "ethers": "^6.16.0",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "form-data": "^4.0.5",
    "helmet": "^8.1.0",
    "ipfs-http-client": "^60.0.1",
    "jsonwebtoken": "^9.0.3",
    "kleur": "^4.1.5",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nocache": "^4.0.0",
    "socket.io": "^4.8.3",
    "ua-parser-js": "^2.0.8",
    "web3": "^4.16.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@prisma/client": "^5.22.0",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.3",
    "@types/socket.io": "^3.0.2",
    "@types/ua-parser-js": "^0.7.39",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^11.0.0",
    "javascript-obfuscator": "^5.1.0",
    "nodemon": "^3.1.11",
    "prisma": "^5.22.0",
    "rimraf": "^6.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
